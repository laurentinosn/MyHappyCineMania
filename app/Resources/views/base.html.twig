        <script src="{{asset('bundles/myhappycinemania/js/jquery.gmap.min.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/circularnav.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/jquery.sticky.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/jflickrfeed.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/imagesloaded.pkgd.min.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/waypoints.min.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/spectrum.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/switcher.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/custom.js')}}"></script>
