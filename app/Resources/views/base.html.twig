        <link rel="stylesheet" href="css/ie.css">
        <![endif]-->
    </head>
    <body class="home">
        <div class="wrap">
            <!-- Header Start -->
            <header id="header">
            {% block top_bar %}
                {% include 'MyHappyCineManiaBundle:Default:top_bar.html.twig' %}
            {% endblock %}
            {% block menu%}
                {% render(controller("MyHappyCineManiaBundle:Default:menu")) %}
            {% endblock %}
            </header>
            <!-- Header End --> 
            <!-- Content Start -->
            <div id="main">
                <!-- Slider Start-->
            {% include 'MyHappyCineManiaBundle:Default:full_banner_container.html.twig' %}
                <!-- Slider End--> 
                <!-- Slogan Start-->
                <div class="slogan bottom-pad-small">
                    <div class="container">
                        <div class="row">
                            <div class="slogan-content">
                                <div class="col-lg-9 col-md-9">
                                    <h2 class="slogan-title">O Melhor e mais alegre do Cinema</h2>
                                </div>
                                {#<div class="col-lg-3 col-md-3">
                                    <div class="get-started">
                                        <button class="btn btn-special btn-color pull-right">Get Started Now</button>
                                    </div>
                                </div>#}
                                <div class="clearfix"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Slogan End--> 
                <!-- Main Content start-->
                <div class="main-content">
                    <div class="container">
                        {% include 'MyHappyCineManiaBundle:Default:cinema_destaque.html.twig' %}
                    </div>
                </div>
                {% include 'MyHappyCineManiaBundle:Default:filmes_destaques.html.twig' %}
                <!-- Main Content end--> 
                
                <!-- Our Clients Start-->
                {% include 'MyHappyCineManiaBundle:Default:publicidades.html.twig' %}
                <!-- Our Clients End -->  
            </div>
            <!-- Content End -->
            <!-- Footer Start -->
        {% include 'MyHappyCineManiaBundle:Default:rodape.html.twig' %}
            <!-- Footer Bottom End --> 
            <!-- Scroll To Top --> 
            <a href="#" class="scrollup"><i class="icon-angle-up"></i></a>
        </div>
        <!-- Wrap End -->
        <!-- The Scripts -->
        <script src="{{asset('bundles/myhappycinemania/js/jquery.min.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/bootstrap.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/jquery.parallax.js')}}"></script> 
        <script src="{{asset('bundles/myhappycinemania/js/modernizr-2.6.2.min.js')}}"></script> 
        <script src="{{asset('bundles/myhappycinemania/js/revolution-slider/js/jquery.themepunch.revolution.min.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/jquery.nivo.slider.pack.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/jquery.prettyPhoto.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/superfish.js')}}"></script>
        <script src="{{asset('bundles/myhappycinemania/js/tweetMachine.js')}}"></script>
